<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="br.ufrn.ct.cronos.entity.Chave" table="chave">
    	<id name="id" column="id_chave" type="java.lang.Long">
        	<generator class="identity" />
      	</id>

      	<property name="codigo" column="codigo_chave" type="java.lang.String" />
      	<property name="descricao" column="descricao_chave" type="java.lang.String" />
      	<property name="idSala" column="id_sala" type="java.lang.Long" />
      	
	</class>
</hibernate-mapping>
