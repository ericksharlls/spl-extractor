<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="br.ufrn.ct.cronos.entity.PermissaoUsuario" table="permissao_usuario">
    	<composite-id>
    		<key-property name="idPapel" column="id_papel" type="java.lang.Long" />
    		<key-property name="idUsuario" column="id_usuario" type="java.lang.Long" />
    	</composite-id>
      	
	</class>
</hibernate-mapping>
