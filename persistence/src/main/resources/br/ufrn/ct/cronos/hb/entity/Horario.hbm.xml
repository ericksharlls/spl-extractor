<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="br.ufrn.ct.cronos.entity.Horario" table="horario">
    	<id name="id" column="id_horario" type="java.lang.Long">
        	<generator class="identity" />
      	</id>

      	<property name="turno" column="turno" type="java.lang.String" />
      	<property name="horario" column="horario" type="java.lang.Integer" />
      	<property name="inicioHorario" column="inicio_horario" type="java.util.Date" />
      	<property name="terminoHorario" column="termino_horario" type="java.util.Date" />
      	<property name="horarioIntermediario" column="horario_intermediario" type="java.util.Date" />
      	<property name="inicioHorarioAbsoluto" column="inicio_horario_absoluto" type="java.util.Date" />
      	<property name="terminoHorarioAbsoluto" column="termino_horario_absoluto" type="java.util.Date" />
      	
	</class>
</hibernate-mapping>
