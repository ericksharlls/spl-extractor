<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
    <class name="br.ufrn.ct.cronos.entity.ParametrosRelatorios" table="parametros_relatorios">
        <id name="id" column="id_parametros_relatorios" type="java.lang.Long">
            <generator class="identity" />
        </id>

        <property name="identificador" column="identificador_parametro_relatorios" type="java.lang.String" />
        <property name="texto" column="texto_parametro_relatorios" type="java.lang.String" />
        
    </class>
</hibernate-mapping>
